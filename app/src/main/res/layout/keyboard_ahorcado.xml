<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/hebrwKeyboardView" android:layout_width="match_parent"
	android:layout_alignParentBottom="true" android:layout_below="@+id/spacecrucigrama"
	android:orientation="vertical" android:background="#252625"  android:layout_centerVertical="true"
	android:visibility="visible" android:layout_height="wrap_content">
	<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:orientation="vertical" android:layout_alignParentBottom="true"
		android:clipChildren="true">
		<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:padding="0sp">
			<TableRow android:layout_width="match_parent"
				android:layout_height="fill_parent" android:padding="0sp">

				<LinearLayout
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:baselineAligned="true"
					android:fitsSystemWindows="true">

					<Button
						android:id="@+id/xQ"
						android:layout_width="30sp"
						android:layout_height="fill_parent"
						android:padding="0sp"
						android:tag="q"
						android:text="q"
						android:textAllCaps="false"
						android:textColor="#000"
						android:textStyle="bold" />

					<Button
						android:id="@+id/xW"
						android:layout_width="30sp"
						android:layout_height="fill_parent"
						android:layout_gravity="left"
						android:padding="0sp"
						android:soundEffectsEnabled="true"
						android:tag="w"
						android:text="w"
						android:textAllCaps="false"
						android:textColor="#000"
						android:textStyle="bold" />

					<Button
						android:id="@+id/xE"
						android:layout_width="30sp"
						android:layout_height="fill_parent"
						android:layout_gravity="left"
						android:fitsSystemWindows="true"
						android:padding="0sp"
						android:soundEffectsEnabled="true"
						android:tag="e"
						android:text="e"
						android:textAllCaps="false"
						android:textColor="#000"
						android:textStyle="bold" />

					<Button
						android:id="@+id/xR"
						android:layout_width="30sp"
						android:layout_height="fill_parent"
						android:layout_gravity="center_horizontal"
						android:ellipsize="marquee"
						android:fitsSystemWindows="true"
						android:soundEffectsEnabled="true"
						android:tag="r"
						android:text="r"
						android:textAllCaps="false"
						android:textColor="#000"
						android:textStyle="bold" />

					<Button
						android:id="@+id/xT"
						android:layout_width="30sp"
						android:layout_height="fill_parent"
						android:layout_gravity="center_horizontal"
						android:ellipsize="marquee"
						android:fitsSystemWindows="true"
						android:soundEffectsEnabled="true"
						android:tag="t"
						android:text="t"
						android:textAllCaps="false"
						android:textColor="#000"
						android:textStyle="bold" />

					<Button
						android:id="@+id/xY"
						android:layout_width="30sp"
						android:layout_height="fill_parent"
						android:layout_gravity="center"
						android:ellipsize="marquee"
						android:fitsSystemWindows="true"
						android:soundEffectsEnabled="true"
						android:tag="y"
						android:text="y"
						android:textAllCaps="false"
						android:textColor="#000"
						android:textStyle="bold" />

					<Button
						android:id="@+id/xU"
						android:layout_width="30sp"
						android:layout_height="fill_parent"
						android:layout_gravity="center_horizontal"
						android:ellipsize="marquee"
						android:fitsSystemWindows="true"
						android:soundEffectsEnabled="true"
						android:tag="u"
						android:text="u"
						android:textAllCaps="false"
						android:textColor="#000"
						android:textStyle="bold" />

					<Button
						android:id="@+id/xI"
						android:layout_width="30sp"
						android:layout_height="fill_parent"
						android:layout_gravity="center_horizontal"
						android:ellipsize="marquee"
						android:fitsSystemWindows="true"
						android:soundEffectsEnabled="true"
						android:tag="i"
						android:text="i"
						android:textAllCaps="false"
						android:textColor="#000"
						android:textStyle="bold" />

					<Button
						android:id="@+id/xO"
						android:layout_width="30sp"
						android:layout_height="fill_parent"
						android:layout_gravity="center_horizontal"
						android:ellipsize="marquee"
						android:fitsSystemWindows="true"
						android:soundEffectsEnabled="true"
						android:tag="o"
						android:text="o"
						android:textAllCaps="false"
						android:textColor="#000"
						android:textStyle="bold" />

					<Button
						android:id="@+id/xP"
						android:layout_width="30sp"
						android:layout_height="fill_parent"
						android:layout_gravity="center_horizontal"
						android:ellipsize="marquee"
						android:fitsSystemWindows="true"
						android:soundEffectsEnabled="true"
						android:tag="p"
						android:text="p"
						android:textAllCaps="false"
						android:textColor="#000"
						android:textStyle="bold" />
					<Button
						android:id="@+id/espacio"
						android:layout_width="30sp"
						android:layout_height="fill_parent"
						android:layout_gravity="center_horizontal"
						android:ellipsize="marquee"
						android:fitsSystemWindows="true"
						android:soundEffectsEnabled="true"
						android:visibility="gone"
						android:tag=""
						android:text=""
						android:textAllCaps="false"
						android:textColor="#000"
						android:textStyle="bold" />
				</LinearLayout>
			</TableRow>
			<TableRow android:layout_width="fill_parent"
				android:layout_height="fill_parent">
				<LinearLayout
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:baselineAligned="true"
					android:fitsSystemWindows="true">
					<Button android:layout_alignWithParentIfMissing="true"
						android:soundEffectsEnabled="true" android:id="@+id/xA"
						android:layout_width="30sp" android:layout_centerHorizontal="true"
						android:tag="a" android:layout_gravity="center_horizontal|center_vertical|center"
						android:textColor="#000" android:textStyle="bold"
						android:layout_height="fill_parent" android:text="a"
						android:layout_alignParentLeft="true" android:fitsSystemWindows="true" android:textAllCaps="false"/>
					<Button android:layout_centerHorizontal="true"
						android:soundEffectsEnabled="true" android:layout_toRightOf="@id/xA"
						android:id="@+id/xS" android:layout_width="30sp" android:tag="s"
						android:layout_gravity="center_horizontal|center_vertical|center"
						android:layout_height="fill_parent" android:text="s"
						android:textColor="#000" android:textStyle="bold"
						android:layout_alignWithParentIfMissing="true"
						android:fitsSystemWindows="true" android:textAllCaps="false"/>
					<Button android:layout_alignWithParentIfMissing="true"
						android:layout_centerHorizontal="true"
						android:soundEffectsEnabled="true" android:id="@+id/xD"
						android:layout_toRightOf="@id/xS" android:layout_width="30sp"
						android:layout_gravity="center_horizontal|center_vertical|center"
						android:textColor="#000" android:textStyle="bold" android:tag="d"
						android:layout_height="fill_parent" android:text="d"
						android:fitsSystemWindows="true" android:textAllCaps="false"/>
					<Button android:layout_alignWithParentIfMissing="true"
						android:layout_centerHorizontal="true"
						android:soundEffectsEnabled="true" android:id="@+id/xF"
						android:layout_toRightOf="@id/xD" android:layout_width="30sp"
						android:tag="f" android:layout_gravity="center_horizontal|center_vertical|center"
						android:layout_height="fill_parent" android:text="f"
						android:textColor="#000" android:textStyle="bold"
						android:fitsSystemWindows="true" android:textAllCaps="false"/>
					<Button android:layout_alignWithParentIfMissing="true"
						android:layout_centerHorizontal="true"
						android:soundEffectsEnabled="true" android:id="@+id/xG"
						android:layout_toRightOf="@id/xF" android:layout_width="30sp"
						android:textColor="#000" android:textStyle="bold"
						android:layout_height="fill_parent" android:layout_gravity="center_horizontal|center_vertical|center"
						android:tag="g" android:text="g" android:fitsSystemWindows="true" android:textAllCaps="false"/>
					<Button android:layout_alignWithParentIfMissing="true"
						android:layout_centerHorizontal="true"
						android:soundEffectsEnabled="true" android:id="@+id/xH"
						android:layout_toRightOf="@id/xG" android:layout_width="30sp"
						android:tag="h" android:layout_height="fill_parent"
						android:textColor="#000" android:textStyle="bold"
						android:layout_gravity="center_horizontal|center_vertical|center"
						android:text="h" android:fitsSystemWindows="true" android:textAllCaps="false"/>
					<Button android:layout_alignWithParentIfMissing="true"
						android:layout_centerHorizontal="true"
						android:soundEffectsEnabled="true" android:id="@+id/xJ"
						android:layout_toRightOf="@id/xH" android:layout_width="30sp"
						android:textColor="#000" android:textStyle="bold"
						android:layout_height="fill_parent" android:layout_gravity="center_horizontal|center_vertical|center"
						android:tag="j" android:text="j" android:fitsSystemWindows="true" android:textAllCaps="false"/>
					<Button android:layout_alignWithParentIfMissing="true"
						android:layout_centerHorizontal="true"
						android:soundEffectsEnabled="true" android:id="@+id/xK"
						android:textColor="#000" android:textStyle="bold"
						android:layout_toRightOf="@id/xJ" android:layout_width="30sp"
						android:layout_height="fill_parent" android:layout_gravity="center_horizontal|center_vertical|center"
						android:tag="k" android:text="k" android:fitsSystemWindows="true" android:textAllCaps="false"/>
					<Button android:layout_alignWithParentIfMissing="true"
						android:layout_centerHorizontal="true"
						android:soundEffectsEnabled="true" android:id="@+id/xL"
						android:textColor="#000" android:textStyle="bold"
						android:layout_toRightOf="@id/xK" android:layout_width="30sp"
						android:layout_height="fill_parent" android:layout_gravity="center_horizontal|center_vertical|center"
						android:tag="l" android:text="l" android:fitsSystemWindows="true" android:textAllCaps="false"/>
					<Button android:layout_alignWithParentIfMissing="true"
						android:layout_centerHorizontal="true"
						android:soundEffectsEnabled="true" android:id="@+id/xS1"
						android:layout_toRightOf="@id/xL" android:layout_width="30sp"
						android:textColor="#000" android:textStyle="bold"
						android:layout_height="fill_parent" android:layout_gravity="center_horizontal|center_vertical|center"
						android:tag="ñ" android:text="ñ" android:fitsSystemWindows="true" android:textAllCaps="false"/>
				</LinearLayout>
			</TableRow>
			<TableRow android:layout_width="fill_parent"
				android:layout_height="fill_parent" android:layout_gravity="fill_vertical"
				android:fitsSystemWindows="true" android:orientation="horizontal">
				<LinearLayout android:layout_width="fill_parent"
					android:layout_height="wrap_content" android:gravity="bottom"
					android:orientation="horizontal">
					<Button android:soundEffectsEnabled="true" android:id="@+id/xS2"
						android:layout_width="50sp" android:layout_gravity="center_horizontal|center_vertical|center"
						android:textColor="#000" android:textStyle="bold"
						android:layout_height="fill_parent" android:text="->" android:tag="->"
						android:fitsSystemWindows="true" android:visibility="invisible"/>
					<Button android:soundEffectsEnabled="true" android:id="@+id/xZ"
						android:layout_width="30sp" android:layout_gravity="center_horizontal|center_vertical|center"
						android:layout_height="fill_parent" android:text="z" android:tag="z"
						android:textColor="#000" android:textStyle="bold"
						android:fitsSystemWindows="true" android:textAllCaps="false"/>
					<Button android:soundEffectsEnabled="true" android:id="@+id/xX"
						android:layout_width="30sp" android:layout_gravity="center_horizontal|center_vertical|center"
						android:tag="x" android:layout_height="fill_parent" android:text="x"
						android:textColor="#000" android:textStyle="bold"
						android:fitsSystemWindows="true" android:textAllCaps="false"/>
					<Button android:soundEffectsEnabled="true" android:id="@+id/xC"
						android:layout_width="30sp" android:layout_gravity="center_horizontal|center_vertical|center"
						android:tag="c" android:layout_height="fill_parent" android:text="c"
						android:textColor="#000" android:textStyle="bold"
						android:fitsSystemWindows="true" android:textAllCaps="false"/>
					<Button android:soundEffectsEnabled="true" android:id="@+id/xV"
						android:layout_width="30sp" android:layout_height="fill_parent"
						android:layout_gravity="center_horizontal|center_vertical|center"
						android:textColor="#000" android:textStyle="bold" android:tag="v"
						android:text="v" android:fitsSystemWindows="true" android:textAllCaps="false"/>
					<Button android:soundEffectsEnabled="true" android:id="@+id/xB"
						android:layout_width="30sp" android:layout_height="fill_parent"
						android:textColor="#000" android:textStyle="bold"
						android:layout_gravity="center_horizontal|center_vertical|center"
						android:tag="b" android:text="b" android:fitsSystemWindows="true" android:textAllCaps="false"/>
					<Button android:soundEffectsEnabled="true" android:id="@+id/xN"
						android:layout_width="30sp" android:layout_height="fill_parent"
						android:layout_gravity="center_horizontal|center_vertical|center"
						android:textColor="#000" android:textStyle="bold" android:tag="n"
						android:text="n" android:fitsSystemWindows="true" android:textAllCaps="false"/>
					<Button android:soundEffectsEnabled="true" android:id="@+id/xM"
						android:layout_width="30sp" android:layout_height="fill_parent"
						android:layout_gravity="center_horizontal|center_vertical|center"
						android:textColor="#000" android:textStyle="bold" android:tag="m"
						android:text="m" android:fitsSystemWindows="true" android:textAllCaps="false"/>
					<Button android:soundEffectsEnabled="true" android:id="@+id/xS3"
						android:layout_width="30sp" android:layout_height="fill_parent"
						android:textColor="#000" android:textStyle="bold" android:tag="é"
						android:layout_gravity="center_horizontal|center_vertical|center"
						android:text="é" android:fitsSystemWindows="true" android:visibility="gone"/>

					<Button
						android:id="@+id/xBack"
						android:layout_width="40dp"
						android:layout_height="fill_parent"
						android:layout_gravity="center_horizontal|center_vertical|center"
						android:background="@drawable/back_high"
						android:fitsSystemWindows="true"
						android:soundEffectsEnabled="true"
						android:tag="back"
						android:visibility="invisible"
						android:textColor="#000"
						android:textStyle="bold" />


				</LinearLayout>
			</TableRow>
			<TableRow android:layout_width="fill_parent"
				android:layout_height="fill_parent" android:fitsSystemWindows="true"
				android:orientation="horizontal">
				<LinearLayout android:layout_width="fill_parent"
					android:layout_height="wrap_content" android:gravity="bottom"
					android:orientation="horizontal">
					<Button android:soundEffectsEnabled="true" android:id="@+id/xUno"
						android:layout_width="30sp" android:layout_height="fill_parent"
						android:textColor="#000" android:textStyle="bold"
						android:layout_gravity="right" android:fitsSystemWindows="true"
						android:tag="1" android:text="1" />
					<Button android:soundEffectsEnabled="true" android:id="@+id/xDos"
						android:textColor="#000" android:textStyle="bold"
						android:layout_width="30sp" android:layout_height="fill_parent"
						android:tag="2" android:text="2" android:fitsSystemWindows="true" />
					<Button android:soundEffectsEnabled="true" android:id="@+id/xTres"
						android:layout_width="30sp" android:layout_height="fill_parent"
						android:textColor="#000" android:textStyle="bold"
						android:layout_gravity="center_horizontal|center_vertical|center"
						android:tag="3" android:text="3" android:fitsSystemWindows="true"/>
					<Button android:soundEffectsEnabled="true" android:id="@+id/xCuatro"
						android:layout_width="30sp" android:layout_height="fill_parent"
						android:layout_gravity="center_horizontal|center_vertical|center"
						android:textColor="#000" android:textStyle="bold" android:tag="4"
						android:text="4" android:fitsSystemWindows="true" />
					<Button android:soundEffectsEnabled="true" android:id="@+id/xCinco"
						android:layout_width="30sp" android:layout_height="fill_parent"
						android:textColor="#000" android:textStyle="bold" android:tag="5"
						android:text="5" android:fitsSystemWindows="true" />
					<Button android:soundEffectsEnabled="true" android:id="@+id/xSeis"
						android:layout_width="30sp" android:layout_height="fill_parent"
						android:textColor="#000" android:textStyle="bold" android:tag="6"
						android:text="6" android:fitsSystemWindows="true" />
					<Button android:soundEffectsEnabled="true" android:id="@+id/xSiete"
						android:layout_width="30sp" android:layout_height="fill_parent"
						android:textColor="#000" android:textStyle="bold" android:tag="7"
						android:text="7" android:fitsSystemWindows="true" />
					<Button android:soundEffectsEnabled="true" android:id="@+id/xOcho"
						android:layout_width="30sp" android:layout_height="fill_parent"
						android:textColor="#000" android:textStyle="bold" android:tag="8"
						android:text="8" android:fitsSystemWindows="true" />
					<Button android:soundEffectsEnabled="true" android:id="@+id/xNueve"
						android:layout_width="30sp" android:layout_height="fill_parent"
						android:textColor="#000" android:textStyle="bold" android:tag="9"
						android:text="9" android:fitsSystemWindows="true" />
					<Button android:soundEffectsEnabled="true" android:id="@+id/xCero"
						android:layout_width="30sp" android:layout_height="fill_parent"
						android:textColor="#000" android:textStyle="bold" android:tag="0"
						android:text="0" android:fitsSystemWindows="true" />


				</LinearLayout>
			</TableRow>
			<TableRow android:layout_width="fill_parent"
				android:layout_height="fill_parent" android:fitsSystemWindows="true"
				android:orientation="horizontal" android:visibility="gone">
				<LinearLayout android:layout_width="fill_parent"
					android:layout_height="wrap_content" android:gravity="bottom"
					android:orientation="horizontal">
					<Button android:soundEffectsEnabled="true" android:id="@+id/xChange"
						android:layout_width="wrap_content" android:layout_height="fill_parent"
						android:textColor="#000" android:textStyle="bold"
						android:layout_gravity="right" android:fitsSystemWindows="true"
						android:paddingLeft="35sp" android:tag="upper" android:background="@drawable/change" android:visibility="gone"/>
					<Button android:soundEffectsEnabled="true" android:id="@+id/xS4"
						android:layout_width="33sp" android:layout_height="fill_parent"
						android:textColor="#000" android:textStyle="bold"
						android:layout_gravity="right" android:fitsSystemWindows="true"
						android:tag="è" android:text="è" android:visibility="invisible"/>
					<Button android:soundEffectsEnabled="true" android:id="@+id/xSpace"
						android:textColor="#000" android:textStyle="bold"
						android:layout_width="75sp" android:layout_height="fill_parent"
						android:tag=" " android:text="|_____|" android:fitsSystemWindows="true" />
					<Button android:soundEffectsEnabled="true" android:id="@+id/xS5"
						android:layout_width="30sp" android:layout_height="fill_parent"
						android:textColor="#000" android:textStyle="bold"
						android:layout_gravity="center_horizontal|center_vertical|center"
						android:tag="û" android:text="û" android:fitsSystemWindows="true" android:visibility="invisible"/>
					<Button android:soundEffectsEnabled="true" android:id="@+id/xS6"
						android:layout_width="30sp" android:layout_height="fill_parent"
						android:layout_gravity="center_horizontal|center_vertical|center"
						android:textColor="#000" android:textStyle="bold" android:tag="î"
						android:text="î" android:fitsSystemWindows="true" android:visibility="invisible"/>
					<Button android:soundEffectsEnabled="true" android:id="@+id/xNum"
						android:layout_width="wrap_content" android:layout_height="fill_parent"
						android:textColor="#000" android:textStyle="bold" android:tag="num"
						android:text="123#" android:fitsSystemWindows="true" />

					<Button
						android:id="@+id/xDone"
						android:layout_width="34dp"
						android:layout_height="match_parent"
						android:layout_gravity="right"
						android:background="@drawable/hide_high"
						android:fitsSystemWindows="true"
						android:soundEffectsEnabled="true"
						android:tag="done"
						android:textColor="#000"
						android:textStyle="bold" />
				</LinearLayout>
			</TableRow>
		</TableLayout>
	</TableLayout>
</RelativeLayout>